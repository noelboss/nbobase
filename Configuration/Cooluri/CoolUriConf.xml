<?xml version="1.0" encoding="utf-8"?>
<cooluri>
	<cache>
		<usecache>1</usecache>
		<params2cool>
			<checkforchangeevery>1</checkforchangeevery>
		</params2cool>
		<cacheparams>0</cacheparams>
		<cool2params>
			<oldlinksvalidfor>30</oldlinksvalidfor>
		</cool2params>
		<pagenotfound>
			<status>HTTP/1.0 404 Not Found</status>
			<behavior type="userfunc">tx_cooluri-&gt;pageNotFound</behavior>
		</pagenotfound>
	</cache>
	<pagepath>
		<title>alias,tx_realurl_pathsegment,nav_title,title,subtitle</title>
		<saveto>id</saveto>
		<default>0</default>
		<userfunc>tx_cooluri-&gt;getPageTitle</userfunc>
		<t3conv>1</t3conv>
	</pagepath>
	<uriparts>
		<part>
			<parameter>tx_nbogallery_gallery[gallery]</parameter>
			<lookindb>
				<to>SELECT title FROM tx_nbogallery_domain_model_gallery WHERE uid=$1 AND deleted=0 AND hidden=0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nxnews_display[year]</parameter>
		</part>
		<part>
			<parameter>tx_nxnews_display[news]</parameter>
			<lookindb>
				<to>SELECT title FROM tx_nxnews_domain_model_news WHERE uid=$1</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
	</uriparts>
	<predefinedparts>
		<part>
			<parameter>cHash</parameter>
		</part>
		<part>
			<parameter>no_cache</parameter>
		</part>
		<part>
			<parameter>tx_nbogallery_gallery[action]</parameter>
		</part>
		<part>
			<parameter>tx_nbogallery_gallery[controller]</parameter>
		</part>
		<part>
			<parameter>tx_nxnews_display[action]</parameter>
		</part>
		<part>
			<parameter>tx_nxnews_display[controller]</parameter>
		</part>
	</predefinedparts>
	<valuemaps>
		<valuemap>
			<parameter>L</parameter>
			<value key="en"></value>
			<value key="en">0</value>
			<value key="de">1</value>
			<value key="pt">2</value>
		</valuemap>
	</valuemaps>


	<paramorder>
		<param>L</param>
	</paramorder>
	
	<!--partorder>
        <part>pagepath</part>
        <part>uriparts</part>
        <part>predefinedparts</part>
        <part>valuemaps</part>
    </partorder-->

	<cooluris>1</cooluris>
</cooluri>
