<?xml version="1.0" encoding="utf-8"?>
<cooluri>
	<cache>
		<usecache>1</usecache>
		<params2cool>
			<checkforchangeevery>1</checkforchangeevery>
		</params2cool>
		<cacheparams>0</cacheparams>
		<cool2params>
			<oldlinksvalidfor>30</oldlinksvalidfor>
		</cool2params>
		<pagenotfound>
			<status>HTTP/1.0 404 Not Found</status>
			<behavior type="userfunc">tx_cooluri-&gt;pageNotFound</behavior>
		</pagenotfound>
	</cache>
	<pagepath>
		<title>alias,tx_realurl_pathsegment,title</title>
		<saveto>id</saveto>
		<default>0</default>
		<userfunc>tx_cooluri-&gt;getPageTitle</userfunc>
		<t3conv>1</t3conv>
	</pagepath>
	<uriparts>
		<part>
			<parameter>tx_nbogallery_gallery[gallery]</parameter>
			<lookindb>
				<to>SELECT CONCAT(tt1.title,IF(tt2.number>1,CONCAT('-',tt2.number),'')) FROM tx_nbogallery_domain_model_gallery as tt1, (SELECT COUNT(*) AS number FROM tx_nbogallery_domain_model_gallery WHERE title=(SELECT title FROM tx_nbogallery_domain_model_gallery WHERE uid=$1)) AS tt2 WHERE tt1.uid=$1 AND tt1.deleted=0 AND tt1.hidden=0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_events[event]</parameter>
			<lookindb>
				<to>SELECT CONCAT(tt1.title,IF(tt2.number>1,CONCAT('-',tt2.number),'')) FROM tx_nboevents_domain_model_event as tt1, (SELECT COUNT(*) AS number FROM tx_nboevents_domain_model_event WHERE title=(SELECT title FROM tx_nboevents_domain_model_event WHERE uid=$1)) AS tt2 WHERE tt1.uid=$1 AND tt1.deleted=0 AND tt1.hidden=0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_events[controller]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_events[action]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_events[location]</parameter>
			<lookindb>
				<to>SELECT CONCAT(tt1.title,IF(tt2.number>1,CONCAT('-',tt2.number),'')) FROM tx_nboevents_domain_model_location as tt1, (SELECT COUNT(*) AS number FROM tx_nboevents_domain_model_location WHERE title=(SELECT title FROM tx_nboevents_domain_model_location WHERE uid=$1)) AS tt2 WHERE tt1.uid=$1 AND tt1.deleted=0 AND tt1.hidden=0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_events[newReservation]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_events[reservation]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_events[newPerson]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_events[e]</parameter>
		</part>

		<part>
			<parameter>tx_nbowishlist_whishlist[wish]</parameter>
			<lookindb>
				<to>
SELECT Concat(tt1.title, IF(tt2.number > 1, Concat('-', tt2.uid), ''))
FROM	tx_nbowishlist_domain_model_wish AS tt1,
		(SELECT uid, Count(*) AS number
		FROM   tx_nbowishlist_domain_model_wish
		WHERE  title = (SELECT title
						FROM   tx_nbowishlist_domain_model_wish
						WHERE  uid = 148)) AS tt2
						WHERE  tt1.uid = 148
						AND tt1.deleted = 0
						AND tt1.hidden = 0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nbowishlist_whishlist[controller]</parameter>
		</part>
		<part>
			<parameter>tx_nbowishlist_whishlist[action]</parameter>
		</part>
		<part>
			<parameter>tx_nbowishlist_whishlist[newParticipation]</parameter>
		</part>
		<part>
			<parameter>tx_nbowishlist_whishlist[participation]</parameter>
		</part>
		<part>
			<parameter>tx_nbowishlist_whishlist[newPerson]</parameter>
		</part>
	</uriparts>
	<predefinedparts>
		<part>
			<parameter>cHash</parameter>
		</part>
		<part>
			<parameter>no_cache</parameter>
		</part>
		<part>
			<parameter>tx_nbogallery_gallery[action]</parameter>
		</part>
		<part>
			<parameter>tx_nbogallery_gallery[controller]</parameter>
		</part>
	</predefinedparts>
	<valuemaps>
		<valuemap>
			<parameter>L</parameter>
			<value key="">0</value>
			<value key="en">1</value>
		</valuemap>
		<valuemap>
			<parameter>tx_nboevents_events[controller]</parameter>
			<value key="">Event</value>
			<value key="">Location</value>
			<value key="anmeldung">Reservation</value>
		</valuemap>
		<valuemap>
			<parameter>tx_nboevents_events[action]</parameter>
			<value key="">show</value>
			<value key="">list</value>
			<value key="neu">new</value>
			<value key="bearbeiten">edit</value>
			<value key="loeschen">delete</value>
		</valuemap>
		<valuemap>
			<parameter>tx_nbowishlist_whishlist[controller]</parameter>
			<value key="">Wish</value>
			<value key="schenken">Participation</value>
		</valuemap>
		<valuemap>
			<parameter>tx_nbowishlist_whishlist[action]</parameter>
			<value key="">show</value>
			<value key="">list</value>
			<value key="neu">new</value>
			<value key="bearbeiten">edit</value>
			<value key="loeschen">delete</value>
		</valuemap>
		<valuemap>
			<parameter>tx_nboevents_events[e][reskey]</parameter>
			<value key="fehler">1</value>
		</valuemap>
	</valuemaps>


	<paramorder>
		<param>L</param>
	</paramorder>

	<!--partorder>
		<part>pagepath</part>
		<part>uriparts</part>
		<part>predefinedparts</part>
		<part>valuemaps</part>
	</partorder-->

	<cooluris>1</cooluris>
</cooluri>
