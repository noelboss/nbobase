<cooluri>
	<cache>
		<usecache>1</usecache>
		<params2cool>
			<checkforchangeevery>1</checkforchangeevery>
		</params2cool>
		<cacheparams>0</cacheparams>
		<cool2params>
			<oldlinksvalidfor>30</oldlinksvalidfor>
		</cool2params>
		<pagenotfound>
			<status>HTTP/1.0 404 Not Found</status>
			<behavior type="userfunc">tx_cooluri-&gt;pageNotFound</behavior>
		</pagenotfound>
	</cache>
	<pagepath>
		<title>alias,tx_realurl_pathsegment,title</title>
		<saveto>id</saveto>
		<default>0</default>
		<userfunc>tx_cooluri-&gt;getPageTitle</userfunc>
		<t3conv>1</t3conv>
	</pagepath>
	<uriparts>
		<part>
			<parameter>tx_nbogallery_gallery[gallery]</parameter>
			<lookindb>
				<to>SELECT title FROM tx_nbogallery_domain_model_gallery WHERE uid=$1 AND deleted=0 AND hidden=0</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>

		<part>
			<parameter>tx_nboevents_courses[course]</parameter>
			<lookindb>
				<to>
					SELECT Concat(tt1.title, IF(tt2.number > 1, Concat('-', tt1.uid), ''))
					FROM	tx_nboevents_domain_model_course AS tt1,
							(SELECT uid, Count(*) AS number
							FROM   tx_nboevents_domain_model_course
							WHERE  title = (SELECT title
											FROM   tx_nboevents_domain_model_course
											WHERE  uid = $1)) AS tt2
											WHERE  tt1.uid = $1
											AND tt1.deleted = 0
											AND tt1.hidden = 0
				</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_courses[event]</parameter>
			<lookindb>
				<to>SELECT CONCAT(c.title,'-',e.uid) FROM tx_nboevents_domain_model_course AS c, tx_nboevents_domain_model_event AS e WHERE e.course = c.uid AND e.deleted=0 AND e.hidden = 0 AND e.uid=$1</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_courses[controller]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_courses[action]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_courses[location]</parameter>
			<lookindb>
				<to>
					SELECT Concat(tt1.title, IF(tt2.number > 1, Concat('-', tt1.uid), ''))
					FROM	tx_nboevents_domain_model_location AS tt1,
							(SELECT uid, Count(*) AS number
							FROM   tx_nboevents_domain_model_location
							WHERE  title = (SELECT title
											FROM   tx_nboevents_domain_model_location
											WHERE  uid = $1)) AS tt2
											WHERE  tt1.uid = $1
											AND tt1.deleted = 0
											AND tt1.hidden = 0
				</to>
				<t3conv>1</t3conv>
			</lookindb>
		</part>
		<part>
			<parameter>tx_nboevents_courses[newReservation]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_courses[reservation]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_courses[newPerson]</parameter>
		</part>
		<part>
			<parameter>tx_nboevents_courses[e]</parameter>
		</part>


